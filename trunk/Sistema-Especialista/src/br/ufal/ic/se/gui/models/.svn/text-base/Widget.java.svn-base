package br.ufal.ic.se.gui.models;

import br.ufal.ic.se.node.TLetter;

public class Widget {
    private TLetter letter;
    private String description;

    public Widget(TLetter letter, String description) {
	this.letter = letter;
	this.description = description;
    }    
    
    public TLetter getLetter() {
	return letter;
    }
    
    public String getDescription() {
	return description;
    }
    
}

